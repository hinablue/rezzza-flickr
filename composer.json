{
    "name": "hinablue/rezzza-flickr",
    "description": "Flickr API Wrapper modified by Hina Chen.",
    "keywords": ["flickr"],
    "type": "standalone",
    "homepage": "https://github.com/hinablue/flickr",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/hinablue/flickr/contributors"
    }],
    "require": {
        "php": ">=5.3.2"
    },
    "suggest": {
        "guzzle/guzzle": "HTTP wrapper"
    },
    "require-dev": {
        "guzzle/http": "3.*"
    },
    "suggest": {
        "guzzle/http": "Guzzle Adapter for http requests"
    },
    "autoload":     {
        "psr-0": {
            "Rezzza\\Flickr": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
